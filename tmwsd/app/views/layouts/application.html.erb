    <meta name="viewport" content="width=device-width, initial-scale=1 maximum-scale=1">
  </head>

  <body>

    <table class="table">
      <tr>
        <td scope="col"><h1><a href="/" class="logo">TMWSD.</a></h1></td>
        <td id="second-td" scope="col"><h5 class="text-muted">This Message Will Self-Destruct.</h5></td>
      </tr>
    </table>


    <div id="body-content">
      <%= yield %>
    </div>

  </body>
</html>


